@prefix : <http://flow.recipes/0.1#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix recipe: <http://schema.org/Recipe#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://www.purl.org/dc/terms/> .

@base <http://flow.recipes/0.1> .

:methodesSchema rdf:type skos:ConceptScheme ;
  dct:title "Flow methodes schema" ;

:beat rdf:type owl:NamedIndividual , :Methode ;
     skos:prefLabel "kloppen"@nl , "beat"@en .

:drop rdf:type owl:NamedIndividual , :Methode ;
    skos:prefLabel "loskloppen"@nl , "drop"@en  .

:mix rdf:type owl:NamedIndividual , :Methode ;
     skos:prefLabel "mengen"@nl , "mix"@en  .
    
:roast rdf:type owl:NamedIndividual , :Methode ;
     skos:prefLabel "roosteren"@nl , "roast"@en  .
    
:grind rdf:type owl:NamedIndividual , :Methode ;
     skos:prefLabel "blenden"@nl , "grind"@en  .