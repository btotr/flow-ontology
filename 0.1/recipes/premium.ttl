premium:hummus
  a flow:Recipe ;
  a owl:NamedIndividual ;
  flow:hasInstructions premium:hummusRoastInstruction ;
  <http://schema.org/prepTime> "PT10M" ;
  rdfs:label "Hummus" ;
.
premium:hummusGrindInstruction
  a flow:Instruction ;
  a owl:NamedIndividual ;
  flow:depVariationInstruction premium:hummusRoastInstruction ;
  flow:hasComponentUnit [
      flow:componentAddition "Â½" ;
      flow:hasComponent fs:lemonJuice ;
      flow:weight 20 ;
    ] ;
  flow:hasComponentUnit [
      flow:hasComponent fs:cayennePepper ;
    ] ;
  flow:hasComponentUnit [
      flow:hasComponent fs:chickPea ;
      flow:weight 250 ;
    ] ;
  flow:hasComponentUnit [
      flow:hasComponent fs:salt ;
    ] ;
  flow:hasMethod fs:grind ;
.
premium:hummusMixInstruction
  a flow:Instruction ;
  a owl:NamedIndividual ;
  flow:depVariationInstruction premium:hummusGrindInstruction ;
  flow:hasComponentUnit [
      flow:componentAddition "x-vierge" ;
      flow:hasComponent fs:oliveOil ;
      flow:weight 20 ;
    ] ;
  flow:hasMethod fs:mix ;
.
premium:hummusRoastInstruction
  a flow:Instruction ;
  a owl:NamedIndividual ;
  flow:directions "regelmatig schudden"@nl ;
  flow:hasComponentUnit [
      flow:hasComponent fs:sesameSeed ;
      flow:weight 40 ;
    ] ;
  flow:hasMethod fs:roast ;
.
